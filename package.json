    "dotenv": "^8.2.0",
    "fastify": "^2.10.0",
    "fastify-postgres": "^2.0.0",
    "jest": "24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
